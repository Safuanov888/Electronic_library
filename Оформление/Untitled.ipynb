      "text/latex": [
       "$\\displaystyle \\frac{1}{3} - \\frac{2^{- n}}{2} + \\frac{2^{- 2 n}}{6}$"
      ],
      "text/plain": [
       "1/3 - 1/(2*2**n) + 1/(6*2**(2*n))"
      ]
     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from sympy import *\n",
    "\n",
    "k, n = symbols('k n')\n",
    "expr = 1/2**n * ( Sum( (k/2**n)**2 - 2 * k/2**n , (k, 1, 2**n-1)) + 2**n - 1 )\n",
    "\n",
    "expand(expr.doit())"
   ]
  }
